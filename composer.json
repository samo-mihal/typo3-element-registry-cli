	"type": "typo3-cms-extension",
	"description": "Helper for registering Typo3 content elements, page types or plugins.",
	"version": "11.5.0",
	"license": "GPL-2.0-or-later",
	"require": {
		"digitalwerk/content_element_registry": ">=10 <12",
        "typo3/cms-core": ">=10 <12",
        "php": ">=7.2.5"
    },
	"replace": {
		"digitalwerk/typo3-element-registry-cli": "self.version"
	},
	"authors": [
		{
			"name": "Samuel Mihal",
			"email": "samuel.mihal@digitalwerk.agency",
			"homepage": "https://digitalwerk.agency/"
		}
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "typo3_element_registry_cli"
		}
	},
    "autoload": {
        "psr-4": {
            "Digitalwerk\\Typo3ElementRegistryCli\\": "Classes"
        }
    }
}
